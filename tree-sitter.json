{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "gcc",
      "camelcase": "GCC",
      "title": "GCC",
      "scope": "source.gcc",
      "file-types": [
        "md",
        "expand",
        "ssa",
        "optimized",
        "pd"
      ],
      "injection-regex": "^gcc$",
      "class-name": "TreeSitterGcc"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "GPL v3.0 or later",
    "description": "Tree-sitter grammars for GCC Internals, including Machine Description, RTL, GIMPLE, and match.pd files.",
    "authors": [
      {
        "name": "Raven Hallsby",
        "email": "raven@hallsby.com",
        "url": "https://raven.hallsby.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/KarlJoad/tree-sitter.gcc"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}